View(PM_06)
PM_06 <- as.data.frame(apply(X = (Get_CT_mean(PM_2006) * (Pop$Population_2006 / sum(Pop$Population_2006, na.rm = T))), 
                             MARGIN = 2, FUN = function(x) { sum(x, na.rm = TRUE) }))
colnames(PM_06) <- "PM_2.5"
